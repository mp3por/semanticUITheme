/*******************************
         Theme Overrides
*******************************/
.ui.label.griffin {
  width: 100%;
  height: 100%;
  background-color: @griffinLightblue;
  color: @white;
  display: flex;
  align-items: center;
  border-top: 1px solid @white;
  border-bottom: 1px solid @white;
  border-radius: 0px;

  padding: 0px;

  .icon {
    width: 2.67142857em;
  }
}